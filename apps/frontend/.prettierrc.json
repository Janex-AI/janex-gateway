{
	"endOfLine": "auto",
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"printWidth": 120,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
	"singleAttributePerLine": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
